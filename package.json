{
  "name": "json-column-parser",
  "version": "1.0.0",
  "description": "JSON parser for several data sources columns",
  "main": "src/index.ts",
  "scripts": {
    "build": "cross-env NODE_ENV=production esbuild src/index.ts --bundle --platform=node",
    "test:env": "cross-env NODE_ENV=test",
    "test": "npm run test:env && jest",
    "test:watch": "npm run test:env && jest --watch",
    "test:lint": "npm run test:env && eslint . --ignore-path .gitignore",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StavPonte11/json-column-parser.git"
  },
  "keywords": [
    "JSON",
    "json",
    "atlas",
    "parser",
    "column",
    "hive",
    "presto",
    "trino",
    "delta",
    "atlas",
    "amundsen"
  ],
  "author": "Stav Ponte",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StavPonte11/json-column-parser/issues"
  },
  "homepage": "https://github.com/StavPonte11/json-column-parser#readme",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "babel": "^6.23.0",
    "babel-plugin-wildcard": "^7.0.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.25",
    "eslint": "^8.10.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "source-map-support": "^0.5.21"
  }
}
